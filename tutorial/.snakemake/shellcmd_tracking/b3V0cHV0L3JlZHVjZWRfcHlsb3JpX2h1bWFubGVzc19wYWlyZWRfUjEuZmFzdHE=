bbduk.sh in1=output/reduced_pylori_R1_paired.fastq in2=output/reduced_pylori_R2_paired.fastq out1=output/reduced_pylori_humanless_paired_R1.fastq out2=output/reduced_pylori_humanless_paired_R2.fastq outs=output/reduced_pylori_humanless_unpaired_R3.fastq k=30 -Xmx4g ref=dmp/small_human.fna mcf=0.5; cat output/reduced_pylori_humanless_single.fastq >> output/reduced_pylori_humanless_unpaired_R3.fastq