bbduk.sh in1=output/003_R1_paired.fastq in2=output/003_R2_paired.fastq out1=output/003_humanless_paired_R1.fastq out2=output/003_humanless_paired_R2.fastq outs=output/003_humanless_unpaired_R3.fastq k=30 -Xmx4g ref=dmp/small_human.fna mcf=0.5; cat output/003_humanless_single.fastq >> output/003_humanless_unpaired_R3.fastq